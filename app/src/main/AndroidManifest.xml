<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.wisn.navigator"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <!--<activity android:name=".activity.RadioButtonViewPagerNavigatorActivity">-->
        <activity android:name=".FirstActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.RadioButtonNavigatorActivity" />
        <activity android:name=".activity.RadioButtonViewPagerNavigatorActivity" />
        <activity android:name=".activity.TabLayoutNavigatorActivity"/>
        <activity android:name=".activity.TabLayoutCustomeViewNavigatorActivity"/>
        <activity android:name=".activity.TextViewNavigatorActivity"/>
        <activity android:name=".activity.TextViewViewPagerNavigatorActivity"/>
        <activity android:name=".activity.NetIconNavigatorActivity"/>
    </application>

</manifest>